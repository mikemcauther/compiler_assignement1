var
  x : int;
  y : int;
  max : int;
begin
  x := 2;
  y := 6;
  do x = y then max := x exit
  od;
  do x <= y then max := y ext // "exit" misspelt
  [] y <= x then max := x exit
  od;
  do x <= y the max := y exit // "then" misspelt
  od;
  do x <= y then max := y exit
  ] y <= x then max := x // "[]" misspelt
  od
end